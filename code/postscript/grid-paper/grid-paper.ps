%!PS-Adobe-1.0

/init {
	72 25.4 div dup scale
	0 setlinecap
	1 setlinewidth	
} def

/verticalPart {
	[/height /n /m /size /y /x] dup length 2 add dict begin {exch def} forall
		0 1 m {
			/i exch def
			i size mul x add y moveto
			0 height rlineto
		} for
	end
} def

/horizontalPart {
	[/width /n /m /size /y /x] dup length 2 add dict begin {exch def} forall
		0 1 n {
			/i exch def
			x i size mul y add moveto
			width 0 rlineto
		} for
	end
} def

/grid {
	[/n /m /size /y /x] dup length 2 add dict begin {exch def} forall
		x y size m n n size mul verticalPart
		x y size m n m size mul horizontalPart
	end
} def

save /cleanslate exch def

init

10 10 10 12 5 grid

stroke 

showpage

cleanslate restore
